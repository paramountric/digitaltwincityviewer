{
  "name": "dtcv-dtcc",
  "version": "0.1.0",
  "description": "DTCV App Template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:types": "supabase gen types typescript --local --schema public,n8n,auth | sed 's/export interface Database {/export type Database = {/' > model/generated/db.ts",
    "platform:start": "pnpm supabase:start && echo 'Waiting for Supabase...' && sleep 3 && pnpm services:start && echo 'Waiting for services...' && sleep 5",
    "platform:stop": "pnpm services:stop && pnpm supabase:stop",
    "services:start": "docker compose -f platform/docker-compose.yaml --env-file platform/.env up -d",
    "services:stop": "docker compose -f platform/docker-compose.yaml down",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "n8n:ensure-api-key": "node scripts/ensure-n8n-api-key.js"
  },
  "dependencies": {
